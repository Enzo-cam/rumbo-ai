<div class=3D"codeContent panelContent pdl">
<pre class=3D"syntaxhighlighter-pre" data-syntaxhighlighter-params=3D"brush=
: java; gutter: false; theme: Confluence" data-theme=3D"Confluence">// src/=
driver-ai/services/carbon-calculator.service.ts

@Injectable()
export class CarbonCalculatorService {
  private readonly CO2_PER_LITER =3D 2.68;  // kg CO2
  private readonly TREES_PER_TON =3D 45.8;   // =C3=A1rboles/ton CO2
 =20
  calculateCO2Emissions(fuelUsedMilliliters: number): number {
    const liters =3D fuelUsedMilliliters / 1000000;
    return liters * this.CO2_PER_LITER;
  }
 =20
  calculateCO2PerKm(co2Total: number, distanceMeters: number): number {
    const km =3D distanceMeters / 1000;
    return co2Total / km;
  }
 =20
  compareToFleetAverage(
    driverCO2PerKm: number,=20
    fleetAverageCO2PerKm: number
  ): {
    percentageDifference: number;
    co2Saved: number;
    ranking: 'ECO_CHAMPION' | 'GOOD' | 'AVERAGE' | 'NEEDS_IMPROVEMENT';
  } {
    const diff =3D ((driverCO2PerKm - fleetAverageCO2PerKm) / fleetAverageC=
O2PerKm) * 100;
   =20
    let ranking: any =3D 'AVERAGE';
    if (diff &lt; -10) ranking =3D 'ECO_CHAMPION';
    else if (diff &lt; 0) ranking =3D 'GOOD';
    else if (diff &gt; 15) ranking =3D 'NEEDS_IMPROVEMENT';
   =20
    return {
      percentageDifference: diff,
      co2Saved: fleetAverageCO2PerKm - driverCO2PerKm,
      ranking
    };
  }
 =20
  generateEquivalences(co2Kg: number) {
    const tons =3D co2Kg / 1000;
   =20
    return {
      trees: Math.round(tons * this.TREES_PER_TON),
      flights: Math.round(co2Kg / 217),  // BA-Mendoza flight
      cars: Math.round(co2Kg / 4600),    // 1 auto promedio anual
    };
  }
}</pre>
